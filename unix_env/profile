# ~/.profile: executed by Bourne-compatible login shells.

export HOME=/root

if [ -f ~/.bashrc ]; then
  . ~/.bashrc
fi

# path set by /etc/profile
# export PATH

export PATH=.:$PATH
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/lib

export COLOR='--color=auto'
export LS_OPTIONS='--color=auto'
